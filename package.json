    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "node-trello": "^1.1.2"
  },
  "devDependencies": {},
  "scripts": {
    "start": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiennq/slack-trello.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiennq/slack-trello/issues"
  },
  "env": {
    "TRELLO_KEY": {
      "description": "trello key",
      "required": true
    },
    "TRELLO_TOKEN": {
      "description": "trello token",
      "required": true
